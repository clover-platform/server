<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="plus.xyc.server.wiki.page.mapper.PageVersionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="plus.xyc.server.wiki.page.entity.dto.PageVersion">
        <id column="id" property="id" />
        <result column="page_id" property="pageId" />
        <result column="version_number" property="versionNumber" />
        <result column="update_time" property="updateTime" />
        <result column="update_user" property="updateUser" />
        <result column="current" property="current" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, page_id, version_number, update_time, update_user, current
    </sql>

</mapper>

<mapper namespace="plus.xyc.server.main.activity.mapper.ActivityLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="plus.xyc.server.main.activity.entity.dto.ActivityLog">
        <id column="id" property="id" />
